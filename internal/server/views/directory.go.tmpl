{{define "content"}}
  <table>
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>
      {{if not .File.IsRoot}}
        <tr>
          <td><a href="{{.File.ParentPath}}">..</a></td>
        <tr>
      {{end}}

      {{range .Files}}
        <tr>
          <td>
            {{.EmojiIcon}} <a href="{{.Path}}">{{.Name}}</a>
          </td>
        </tr>
      {{end}}
    <tbody>
    </tbody>
  </table>
{{end}}
